CICLOS

	add		=	8
	addi	=	5
	and		=	6
	andi	=	5
	b		=	5
	beq		=	5
	beqz	=	5
	bge		=	5
	bgt		=	5
	bgtz	=	5
	ble		=	5
	blez	=	5
	blt		=	5
	bne		=	5
	bnez	=	5
	div		=	5
	divi	=	5
	ld		=	5
	li		=	5
	move	=	5
	mult	=	5
	multi	=	5
